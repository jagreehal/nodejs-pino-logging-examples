{
  "name": "nodejs-pino-logging-examples",
  "version": "1.0.0",
  "description": "Examples of logging with Pino in Node.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev  --inspect --no-notify --transpile-only --respawn -- src/index.ts",
    "lint": "eslint --ext .js,.ts ./src",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "keywords": [
    "nodejs",
    "api",
    "logging",
    "pino"
  ],
  "author": "Jag Reehal",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.8.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pino": "^8.10.0",
    "pino-http": "^8.3.3",
    "pino-pretty": "^9.2.0",
    "ulid": "^2.3.0"
  }
}
